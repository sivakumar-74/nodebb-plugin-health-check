{
    "id": "nodebb-plugin-health-check",
    "name": "NodeBB Health Check",
    "description": "A plugin to provide health check endpoints for NodeBB",
    "url": "https://github.com/sivakumar-74/nodebb-plugin-health-check",
    "hooks": [
        { "hook": "static:app.load", "method": "init" },
